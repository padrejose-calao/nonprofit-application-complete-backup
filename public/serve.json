{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/nonprofit-app.html" },
    { "source": "/nonprofit-app", "destination": "/nonprofit-app.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}

